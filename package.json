{
  "name": "devfile-landing-page",
  "author": "Red Hat",
  "version": "1.0.0",
  "homepage": "https://devfile.github.io/landing-page",
  "scripts": {
    "dev": "next",
    "build": "next build && next export",
    "export": "next export",
    "start": "next start",
    "clean": "rimraf .next/ .nyc_output/ coverage/ docs/ out/ node_modules/",
    "test": "jest & cypress run",
    "test:jest": "jest",
    "test:jest-watch": "jest --watchAll",
    "test:cypress": "concurrently --names 'CYPRESS,SERVER' --prefix-colors 'yellow,blue' \"yarn cypress open\" \"yarn build && yarn start\"",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "format": "prettier --write .",
    "prepare": "husky install",
    "debug-options": "ttsc ./scripts/debugOptions.ts && node -r esm ./scripts/debugOptions.js"
  },
  "dependencies": {
    "@headlessui/react": "^1.4.2",
    "@heroicons/react": "^1.0.5",
    "@patternfly/react-core": "^4.181.1",
    "@patternfly/react-icons": "^4.32.1",
    "@segment/analytics-next": "^1.33.1",
    "@stoplight/json-schema-viewer": "^4.5.0",
    "@stoplight/markdown-viewer": "^5.3.3",
    "@stoplight/mosaic": "^1.15.2",
    "@stoplight/mosaic-code-viewer": "^1.15.2",
    "@szhsin/react-menu": "^2.3.2",
    "asciidoctor": "^2.2.5",
    "highlight.js": "^11.4.0",
    "next": "^12.0.7",
    "next-compose-plugins": "^2.2.1",
    "next-images": "^1.8.4",
    "next-transpile-modules": "^9.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "tslib": "^2.3.1",
    "typescript-is": "^0.19.0"
  },
  "license": "EPL-2.0",
  "devDependencies": {
    "@types/jsdom": "^16.2.14",
    "@types/node": "^17.0.8",
    "@types/react": "^17.0.38",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "eslint": "^8.6.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-next": "^12.0.7",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^25.3.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-unicorn": "^40.0.0",
    "esm": "^3.2.25",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.7",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "ttypescript": "^1.5.13",
    "typescript": "^4.5.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,html,css,json}": "prettier --write"
  },
  "optionalDependencies": {
    "@cypress/code-coverage": "^3.9.10",
    "@testing-library/jest-dom": "^5.15.0",
