{
  "name": "devfile-landing-page",
  "author": "Red Hat",
  "version": "1.0.0",
  "homepage": "https://devfile.github.io/landing-page",
  "scripts": {
    "analyze": "cross-env ANALYZE=true next build",
    "dev": "next",
    "build": "next build && next export",
    "export": "next export",
    "start": "next start",
    "clean": "rimraf .next/ .nyc_output/ coverage/ docs/ out/ node_modules/",
    "cypress:start": "concurrently --names 'CYPRESS,SERVER' --prefix-colors 'yellow,blue' \"yarn cypress open\" \"yarn build && yarn start\"",
    "typedoc:build": "typedoc --tsconfig .",
    "typedoc:start": "npx serve docs",
    "jest:test": "jest --watchAll --verbose",
    "test": "cypress run",
    "lint": "prettier --check .",
    "format": "prettier --write .",
    "prepare": "husky install"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^11.0.1",
    "@patternfly/react-core": "^4.121.1",
    "@patternfly/react-icons": "^4.10.7",
    "next": "^11.0.1",
    "next-compose-plugins": "^2.2.1",
    "next-images": "^1.8.1",
    "next-pwa": "^5.2.21",
    "next-transpile-modules": "^8.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "tslib": "^2.3.0",
    "typescript-is": "^0.18.3"
  },
  "license": "EPL-2.0",
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@types/marked": "^2.0.4",
    "@types/node": "^15.6.1",
    "@types/react": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "babel-plugin-istanbul": "^6.0.0",
    "eslint": "^7.27.0",
    "eslint-config-next": "^11.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-patternfly-react": "^4.1.5",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "husky": "^6.0.0",
    "lint-staged": "^11.1.1",
    "prettier": "2.3.1",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.2.5",
    "typescript": "^4.3.4",
    "webpack": "^5.38.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,html,css,json}": "prettier --write"
  },
  "optionalDependencies": {
    "@cypress/code-coverage": "^3.9.10",
    "@types/jest": "^26.0.24",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.3",
    "babel-jest": "^27.0.6",
    "concurrently": "^6.2.0",
    "cross-env": "^7.0.3",
    "cypress": "^8.2.0",
    "debug": "^4.3.2",
    "enquirer": "^2.3.6",
    "enzyme": "^3.11.0",
    "jest": "^27.0.6",
    "jest-enzyme": "^7.1.2",
    "typedoc": "^0.21.5",
    "typedoc-plugin-merge-modules": "^3.0.0"
  }
}
